<div class="orange-background">
  <div class="logo-container">
    <%= image_tag 'logo_white.png', alt: "alttext" %>
  </div>
  <div class="paws-container">
    <i id="paw-1" class="material-icons">pets</i>
    <i id="paw-2" class="material-icons">pets</i>
    <i id="paw-3" class="material-icons">pets</i>
    <i id="paw-4" class="material-icons">pets</i>
