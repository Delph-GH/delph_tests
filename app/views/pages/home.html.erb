    <i id="paw-6" class="material-icons">pets</i>
    <i id="paw-7" class="material-icons">pets</i>
    <i id="paw-8" class="material-icons">pets</i>
    <i id="paw-9" class="material-icons">pets</i>
    <i id="paw-10" class="material-icons">pets</i>
  </div>
  <div class="login-buttons-container">
    <%= link_to "LOGIN", new_user_session_path, class: "button-white waves-effect"  %>
